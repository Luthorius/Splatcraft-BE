{
  "format_version": "1.10.0",
  "animations": {
    "animation.inkcharge": {
	  "loop": true,
      "timeline": {
        "0.0": "/scoreboard players add @s ink_level 2"
      },
      "animation_length": 0.3
    },
    "animation.inkcharge_high": {
	  "loop": true,
      "timeline": {
        "0.0": "/scoreboard players add @s ink_level 3"
      },
      "animation_length": 0.1
    },
    "animation.inkcharge_max": {
	  "loop": true,
      "timeline": {
        "0.0": "/scoreboard players set @s ink_level 100"
      },
      "animation_length": 0.1
    },
    "animation.inkcharge_stop": {
	  "loop": true,
      "timeline": {
        "0.0": "/scoreboard players set @s charge_speed 0",
        "0.6": "/scoreboard players set @s charge_stop 1"
      },
      "animation_length": 0.7
    },
    "animation.inkcharge_start": {
	  "loop": true,
      "timeline": {
        "0.5": "/scoreboard players set @s charge_speed 1",
        "0.6": "/scoreboard players set @s charge_stop 2"
      },
      "animation_length": 0.7
    },
    "animation.in_ink": {
	  "loop": true,
      "timeline": {
        "0.0": "/effect @s[tag=blue] speed 99999 30 true",
        "0.1": "/effect @s[tag=blue] invisibility 99999 1 true",
        "0.2": "/effect @s[tag=blue] regeneration 99999 3 true",
        "0.3": "/scoreboard players set @s[tag=blue] charge_speed 2"
      },
      "animation_length": 0.4
    },
    "animation.in_ink2": {
	  "loop": true,
      "timeline": {
        "0.0": "/effect @s[tag=orange] speed 99999 30 true",
        "0.1": "/effect @s[tag=orange] invisibility 99999 1 true",
        "0.2": "/effect @s[tag=orange] regeneration 99999 3 true",
        "0.3": "/scoreboard players set @s[tag=orange] charge_speed 2"
      },
      "animation_length": 0.4
    },
    "animation.on_ink": {
	  "loop": true,
      "timeline": {
        "0.5": "/effect @s[tag=orange] slowness 1 3 true",
        "0.9": "/effect @s[tag=orange] poison 1 1 true"
      },
      "animation_length": 1.0
    },
    "animation.on_ink2": {
	  "loop": true,
      "timeline": {
        "0.5": "/effect @s[tag=blue] slowness 1 3 true",
        "0.9": "/effect @s[tag=blue] poison 1 1 true"
      },
      "animation_length": 1.0
    },
    "animation.hurt": {
	  "loop": true,
      "timeline": {
        "0.0": "/tag @p add hurt"
      },
      "animation_length": 0.1
    },
    "animation.hurt_stop": {
	  "loop": true,
      "timeline": {
        "0.0": "/tag @p remove hurt"
      },
      "animation_length": 0.1
    }
  }
}